{
  "include": [
    "src/js2/**/*"
  ],
  "compilerOptions": {
    "alwaysStrict": false,
    "noImplicitAny": false,
    "allowJs": false,
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es6",
    "module": "ES2015",
    "lib": ["dom", "es2015"],
    "jsxFactory": "h",
    "noEmitHelpers": true,
    "importHelpers": true
  }
}
